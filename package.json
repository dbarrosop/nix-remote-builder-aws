{
  "name": "nix-remote-builder-aws",
  "version": "0.0.0",
  "private": true,
  "description": "Github Action to deploy nix remote builders on demand",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write 'src/**/*.ts'",
    "format-check": "prettier --check 'src/**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt -o dist/main lib/main.js && ncc build lib/cleanup.js -o dist/cleanup --source-map --license licenses.txt",
    "all": "npm run build && npm run format && npm run lint && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "@aws-sdk/client-ec2": "^3.241.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.48.0",
    "@vercel/ncc": "^0.36.0",
    "eslint": "^8.31.0",
    "eslint-plugin-github": "^4.6.0",
    "eslint-plugin-jest": "^27.2.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.3.1",
    "js-yaml": "^4.1.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4"
  }
}
